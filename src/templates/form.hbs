    <div>
        <h1>{{name}}</h1>
        <ul>
            <li>Capital: {{capital}}</li>
            <li>Population: {{population}}</li>
            <li>Languages:
                <ul>
                    {{#each languages}}
                    <li>{{name}}</li>
                    {{/each}}
                </ul>
            </li>
        </ul>
        <img src="{{flags.png}}" alt="{{name}}" width="280">
    </div>


